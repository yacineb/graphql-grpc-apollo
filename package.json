{
  "name": "graphql-grpc",
  "version": "0.1.0",
  "description": "GraphQl server using grPC transport layer",
  "main": "index",
  "scripts": {
    "test:server": "nodemon --inspect  dist/test/server.js",
    "test:client": "node dist/test/client.js",
    "build": "tsc && npm run cp-protos",
    "watch": "tsc --w",
    "cp-protos": "copyfiles --flat src/protos/*.proto dist/protos"
  },
  "author": "Yacine Bouraoui",
  "license": "ISC",
  "dependencies": {
    "@grpc/proto-loader": "^0.5.2",
    "graphql": "^14.5.6",
    "grpc": "^1.23.3",
    "mali": "^0.19.0",
    "graphql-tools": "4.0.5"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.138",
    "copyfiles": "^2.1.1",
    "express": "^4.17.1",
    "lodash": "3.10.1",
    "nodemon": "^1.19.2",
    "typescript": "^3.6.3",
    "apollo-server": "^2.9.3"
  }
}
